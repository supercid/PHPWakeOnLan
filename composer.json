{
    "name": "supercid/php-wake-on-lan",
    "description": "Wake on lan target enabled devices by sending magic packets to them from PHP",
    "keywords": [
        "lan",
        "wake",
        "wakeonlan",
        "magicpacket"
    ],
    "homepage": "https://github.com/supercid/php-wake-on-lan",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Diego Gonz√°lez",
            "role": "Developer"
        },
        {
            "name": "Cid Lopes",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-sockets": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^7.0",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "SuperCid\\PHPWakeOnLan\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SuperCid\\PHPWakeOnLan\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
